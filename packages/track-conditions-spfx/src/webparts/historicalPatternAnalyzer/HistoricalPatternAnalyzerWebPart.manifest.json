{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "b4a9c3e2-5d8f-4a7b-9c1e-3f2a8b7d4e5c",
  "alias": "HistoricalPatternAnalyzerWebPart",
  "componentType": "WebPart",
  "version": "*",
  "manifestVersion": 2,
  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart", "TeamsPersonalApp", "TeamsTab", "SharePointFullPage"],
  "supportsThemeVariants": true,

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // GRNSW Tools
    "group": { "default": "GRNSW Tools" },
    "title": { "default": "Historical Pattern Analyzer" },
    "description": { "default": "Analyzes historical weather patterns to identify optimal racing conditions" },
    "officeFabricIconFontName": "AnalyticsReport",
    "properties": {
      "refreshInterval": 5,
      "defaultTimeRange": "7days",
      "defaultTracks": [],
      "viewMode": "standard",
      "enableAlerts": true,
      "optimalScoreThreshold": 85,
      "volatilityThreshold": 20,
      "cacheMinutes": 60,
      "maxRecords": 5000,
      "debugMode": false
    }
  }]
}